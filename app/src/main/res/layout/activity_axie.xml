<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="16dp"
    tools:context=".Axie">


    <Button
        android:id="@+id/refresh_Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Refresh "
        android:backgroundTint="@color/UIColor"
        android:textSize="12sp"
        android:textColor="@color/textcolor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.92"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/input_address"
        app:layout_constraintVertical_bias="0.031" />

    <TextView
        android:id="@+id/todaySlp_textView"
        android:layout_width="90dp"
        android:layout_height="32dp"
        android:text="Today's Slp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.737" />

    <TextView
        android:id="@+id/name_textView"
        android:layout_width="50dp"
        android:layout_height="29dp"
        android:text="Name:"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address_showText"
        app:layout_constraintVertical_bias="0.087" />

    <TextView
        android:id="@+id/totalSlp_textView"
        android:layout_width="70dp"
        android:layout_height="29dp"
        android:text="TotalSlp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.351"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address_showText"
        app:layout_constraintVertical_bias="0.084" />

    <TextView
        android:id="@+id/managerSlp_textView"
        android:layout_width="75dp"
        android:layout_height="29dp"
        android:text="Scholar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.926"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address_showText"
        app:layout_constraintVertical_bias="0.084" />

    <TextView
        android:id="@+id/scholarSlp_textView"
        android:layout_width="75dp"
        android:layout_height="29dp"
        android:text="Manager"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.633"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address_showText"
        app:layout_constraintVertical_bias="0.084" />

    <TextView
        android:id="@+id/address_showText"
        android:layout_width="280dp"
        android:layout_height="50dp"
        android:text="Nan"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toEndOf="@+id/address_textView"
        app:layout_constraintTop_toBottomOf="@+id/find_addressButton"
        app:layout_constraintVertical_bias="0.032" />

    <Button
        android:id="@+id/find_addressButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Find Address"
        android:backgroundTint="@color/UIColor"
        android:textColor="@color/textcolor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.096"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/input_address"
        app:layout_constraintVertical_bias="0.031" />

    <TextView
        android:id="@+id/address_textView"
        android:layout_width="56dp"
        android:layout_height="31dp"
        android:text="Address:"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/find_addressButton"
        app:layout_constraintVertical_bias="0.031" />

    <TextView
        android:id="@+id/mmr_textView"
        android:layout_width="90dp"
        android:layout_height="32dp"
        android:text="MMR:"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.74" />

    <TextView
        android:id="@+id/name_showText"
        android:layout_width="70dp"
        android:layout_height="32dp"
        android:text="Nan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name_textView"
        app:layout_constraintVertical_bias="0.027" />

    <TextView
        android:id="@+id/mmr_showText"
        android:layout_width="90dp"
        android:layout_height="32dp"
        android:text="Nan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mmr_textView"
        app:layout_constraintVertical_bias="0.13" />

    <TextView
        android:id="@+id/totalSlp_showText"
        android:layout_width="70dp"
        android:layout_height="32dp"
        android:text="Nan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.351"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/totalSlp_textView"
        app:layout_constraintVertical_bias="0.029" />

    <TextView
        android:id="@+id/managerSlp_showText"
        android:layout_width="70dp"
        android:layout_height="32dp"
        android:text="Nan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.627"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/totalSlp_textView"
        app:layout_constraintVertical_bias="0.029" />

    <TextView
        android:id="@+id/scholarSlp_showText"
        android:layout_width="70dp"
        android:layout_height="32dp"
        android:text="Nan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.917"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/totalSlp_textView"
        app:layout_constraintVertical_bias="0.029" />

    <TextView
        android:id="@+id/yesterdayslp_textView"
        android:layout_width="95dp"
        android:layout_height="32dp"
        android:text="Yesterday's Slp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.737" />

    <TextView
        android:id="@+id/yesterdaySlp_showText"
        android:layout_width="90dp"
        android:layout_height="32dp"
        android:text="Nan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yesterdayslp_textView"
        app:layout_constraintVertical_bias="0.13999999" />

    <TextView
        android:id="@+id/todaySlp_showText"
        android:layout_width="90dp"
        android:layout_height="32dp"
        android:text="Nan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/todaySlp_textView"
        app:layout_constraintVertical_bias="0.134" />

    <EditText
        android:id="@+id/input_address"
        android:layout_width="360dp"
        android:layout_height="60dp"
        android:hint="ronin:0xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
        android:inputType="textNoSuggestions"
        android:maxLength="46"
        android:textColor="@color/textcolor"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.100000024" />

    <TextView
        android:id="@+id/slpValue_Total"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="₱"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.356"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/totalSlp_showText"
        app:layout_constraintVertical_bias="0.07999998" />

    <TextView
        android:id="@+id/slpValue_Manager"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="₱"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.626"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/managerSlp_showText"
        app:layout_constraintVertical_bias="0.083" />

    <TextView
        android:id="@+id/slpValue_Scholar"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="₱"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scholarSlp_showText"
        app:layout_constraintVertical_bias="0.083" />

    <TextView
        android:id="@+id/slp_textValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SLP:"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/input_address"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.743"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.4" />

    <TextView
        android:id="@+id/slp_showValue"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="₱"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/input_address"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.954"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.41000003" />

    <ProgressBar
        android:id="@+id/loadingData"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/todaySlp_showText" />

</androidx.constraintlayout.widget.ConstraintLayout>